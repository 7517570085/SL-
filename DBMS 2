PRACTICAL NO 2 DBMS

mysql> DROP DATABASE IF EXISTS practical;
Query OK, 0 rows affected, 1 warning (0.02 sec)

mysql> CREATE DATABASE practical;
Query OK, 1 row affected (0.01 sec)

mysql> use practical;
Database changed
mysql> CREATE TABLE student (
    -> id INT PRIMARY KEY AUTO_INCREMENT,
    -> admission_no INT NOT NULL UNIQUE,
    ->  first_name VARCHAR(45) NOT NULL,
    ->   last_name VARCHAR(45) NOT NULL,
    ->  age INT,
    -> city VARCHAR(25) NOT NULL
    -> );
Query OK, 0 rows affected (0.11 sec)

mysql> CREATE TABLE Fee (
    -> Fee_id INT PRIMARY KEY AUTO_INCREMENT,
    ->  admission_no INT NOT NULL,
    -> course VARCHAR(100) NOT NULL,
    ->   amount_paid INT,
    ->  CONSTRAINT fk_fee_student FOREIGN KEY (admission_no) REFERENCES Student(admission_no)
    -> );
Query OK, 0 rows affected (0.10 sec)

mysql> INSERT INTO Student(admission_no, first_name, last_name, age, city) VALUES
    -> (1001,'Shital','Gayke',18,'Sinnar'),
    -> (1002,'Sakshi','More',19,'Nashik'),
    -> (1003,'Nidhi','Jadhav',17,'Nashik'),
