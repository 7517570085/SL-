mysql> INSERT INTO Fee(admission_no, course, amount_paid) VALUES
    ->  (1001,'Android',10000),
    -> (1002,'Data Science',15000),
    -> (1003,'SQL',18000),
    -> (1004,'Python',20000),
    -> (1005,'Java',8000),
    -> (1010,'Machine Learning',25000),
    -> (1011,'Cyber Security',22000);
Query OK, 7 rows affected (0.03 sec)
